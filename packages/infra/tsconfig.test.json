{
  "extends": "./tsconfig.json",
  "include": [
    "test/**/*"
  ],
  "compilerOptions": {
    "types": ["node", "jest"],
    "typeRoots": ["./node_modules/@types", "../../node_modules/@types"],
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true
  }
}